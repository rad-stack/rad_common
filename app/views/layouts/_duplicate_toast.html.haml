.position-fixed.bottom-0.right-0.p-3.bottom-toast#duplicate-toast{ data: { model: model } }
  .toast.hide#live-toast{ 'aria-atomic': 'true', 'aria-live' => 'assertive',
                         'data-delay': '6000', role: 'alert' }
    .toast-header
      %strong.mr-auto#duplicate-toast-header
      %button.ml-2.mb-1.close{ 'aria-label': 'Close', 'data-dismiss': 'toast', type: 'button' }
        %span{ 'aria-hidden': 'true' } Ã—
    .toast-body
      = link_to 'View Details', '#duplicate-card', class: 'btn btn-sm btn-warning dup_record_btn'
